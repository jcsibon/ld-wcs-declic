									<xsd:documentation>Metrage, Installation, SAV, ...</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="typeEtape">
											<xsd:annotation>
												<xsd:documentation>Metrage, Installation, SAV, 010, ...</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string"/>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="numeroEtape">
											<xsd:annotation>
												<xsd:documentation>No d'Installation</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="25"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="intervenantEtape" minOccurs="0" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Artisan Poseur, ...</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:complexContent>
													<xsd:extension base="coordonneesType">
														<xsd:sequence>
															<xsd:element name="typeIntervenant">
																<xsd:annotation>
																	<xsd:documentation>Interne ou externe</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:minLength value="1"/>
																		<xsd:maxLength value="25"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
															<xsd:element name="codeIntervenant">
																<xsd:annotation>
																	<xsd:documentation>Matricule ou code externe</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:minLength value="1"/>
																		<xsd:maxLength value="25"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
															<xsd:element name="codeClientPoseur" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>Si le poseur est client</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:minLength value="1"/>
																		<xsd:maxLength value="14"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
														</xsd:sequence>
													</xsd:extension>
												</xsd:complexContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="dateDebut" type="dateType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Date de debut  sur l'etape</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="dateFin" type="dateType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Date de fin sur l'etape</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="clientFacture" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Client facture, signataire pour K par K</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:complexContent>
										<xsd:extension base="clientType">
											<xsd:sequence minOccurs="0">
												<xsd:element name="typeSalarie">
													<xsd:annotation>
														<xsd:documentation>0 = pas salarie Lapeyre, 1 = Salarie Lapeyre, 2 = salarie Groupe</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:length value="1"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="reglement" minOccurs="0" maxOccurs="unbounded">
													<xsd:annotation>
														<xsd:documentation>Reglement, optionnel</xsd:documentation>
													</xsd:annotation>
													<xsd:complexType>
														<xsd:complexContent>
															<xsd:extension base="reglementType"/>
														</xsd:complexContent>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="financement" minOccurs="0">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="codeFinancement">
																<xsd:annotation>
																	<xsd:documentation>Code du financement</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:minLength value="1"/>
																		<xsd:maxLength value="15"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
															<xsd:element name="codeFamilleFinancement" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>Code du financement</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:minLength value="1"/>
																		<xsd:maxLength value="15"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
															<xsd:element name="codeOrganismeFinancement" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>Code de l'organisme</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:minLength value="1"/>
																		<xsd:maxLength value="15"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
															<xsd:element name="montantFinancement" type="xsd:decimal" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>Montant Financement</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="echeanceFinancement" type="dateType" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>Date prevue de fin ?</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="carteSalarie" minOccurs="0">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="20"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="compteProjet" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>0 ou 1</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:integer">
															<xsd:minInclusive value="0"/>
															<xsd:maxInclusive value="1"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
										</xsd:extension>
									</xsd:complexContent>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="clientDonneurdOrdres" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>pour GAM, celui qui passe sa commande, PointP, ...</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:complexContent>
										<xsd:extension base="partenaireType"/>
									</xsd:complexContent>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="clientLivre" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Coordonnees de la livraison, obligatoires</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:complexContent>
										<xsd:extension base="clientType">
											<xsd:sequence minOccurs="0">
												<xsd:element name="livraison" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Caracteristiques de la livraison</xsd:documentation>
													</xsd:annotation>
													<xsd:complexType>
														<xsd:complexContent>
															<xsd:extension base="livraisonType"/>
														</xsd:complexContent>
													</xsd:complexType>
												</xsd:element>
											</xsd:sequence>
										</xsd:extension>
									</xsd:complexContent>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="clientFinal" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Avec les informations de livraison au client final</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:complexContent>
										<xsd:extension base="partenaireType">
											<xsd:sequence minOccurs="0">
												<xsd:element name="numeroCommandeClientFinal">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="20"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="priseDeRendezVous" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>OUI / NON</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:enumeration value="Oui"/>
															<xsd:enumeration value="Non"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="rendezVous" type="livraisonType" minOccurs="0"/>
											</xsd:sequence>
										</xsd:extension>
									</xsd:complexContent>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="clientChantier" type="clientType" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Coordonnees du chantier</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="dossierTechniqueEntete" type="cameleonEnteteType" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Tables ISCLCOM de Cameleon</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="infosAdresseSecondaire" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Si adresse secondaire renseignee</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="codePostalAdrSec">
											<xsd:annotation>
												<xsd:documentation>Code postal de l'adresse secondaire</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="12"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="villeAdrSec" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Ville de l'adresse secondaire</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="25"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="codePaysAdrSec" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Code pays de l'adresse secondaire</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="3"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="commentaire" type="commentaireType" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Commentaires d'en-tete de document</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="infosWeb" type="infosWebType" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ligneDocument" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Lignes de commande, contrat, ...</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="actionLigne" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Creation, Modification, Suppression</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:complexContent>
										<xsd:extension base="actionType"/>
									</xsd:complexContent>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="numeroLigneDocument">
								<xsd:annotation>
									<xsd:documentation>No de ligne de la commande</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:integer"/>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="article">
								<xsd:annotation>
									<xsd:documentation>References de l'article</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:complexContent>
										<xsd:extension base="articleType">
											<xsd:sequence minOccurs="0">
												<xsd:element name="gencod" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Code EAN</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="13"/>
															<xsd:maxLength value="14"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
										</xsd:extension>
									</xsd:complexContent>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosPromotionLigne" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="codePromotionAppliquee">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="45"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="typePromotion" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Simple, Conditionnelle</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="3"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="libellePromotionAppliquee" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="45"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosStatutLigne" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="codeStatutLigne" type="xsd:string"/>
										<xsd:element name="libelleStatutLigne" type="xsd:string"/>
										<xsd:element name="dateStatutLigne" type="dateType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>date de modification du statu de la ligne</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosDisposLigne" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="dateMADMagasin" type="dateType" minOccurs="0"/>
										<xsd:element name="dateLivraison" type="dateType" minOccurs="0"/>
										<xsd:element name="delaiReception" type="xsd:integer" minOccurs="0"/>
										<xsd:element name="delaiOption" type="xsd:integer" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="valorisation" type="valorisationType" minOccurs="0" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Valorisation, YC Transport, optionnel</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="dossierTechniqueLigne" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Tables ISCLLIG, ISCLDET, ISCLDT, ISCAVAAN, SCPC de Cameleon</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="litige" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>references a un litige</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="referenceLitige" type="xsd:string"/>
										<xsd:element name="codeFournisseur" type="xsd:string" minOccurs="0"/>
										<xsd:element name="numeroCommandeFrs" type="xsd:string" minOccurs="0"/>
										<xsd:element name="numeroCommandeDO" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="descriptifProduit" type="xsd:string" minOccurs="0"/>
							<xsd:element name="commentaire" type="commentaireType" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Commentaires de ligne</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ligneMontants" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Lignes de montant (K par K)</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="numeroLigneMontant">
								<xsd:annotation>
									<xsd:documentation>No de ligne de la commande</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:integer">
										<xsd:minInclusive value="1"/>
										<xsd:maxInclusive value="999"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="dateModification" type="dateType"/>
							<xsd:element name="valorisation" type="valorisationType" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Valorisation, YC Transport, optionnel</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="motif" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Commentaires lies a la ligne de montant</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:minLength value="1"/>
										<xsd:maxLength value="300"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="codeMotifResiliation" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Code motif de resiliation</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:minLength value="1"/>
										<xsd:maxLength value="10"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="piedDocument">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="valorisation" type="valorisationType" minOccurs="0" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Valorisation, YC Transport, optionnel</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ecartsCotation" minOccurs="0" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Ecarts de valorisation par le Vendeur ou le Service (K par K)</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="typeEcartCotation" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>V(endeur), S(ervice)</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:length value="1"/>
													<xsd:enumeration value="V"/>
													<xsd:enumeration value="S"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="tauxEcart" type="xsd:decimal" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>% Ecart</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="montantEcart" type="xsd:decimal" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Montant </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="actionType">
		<xsd:annotation>
			<xsd:documentation>Modele pour les actions sur la facture</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="codeAction">
				<xsd:annotation>
					<xsd:documentation>CRE, MOD, SUP, ...</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="CRE"/>
						<xsd:enumeration value="MOD"/>
						<xsd:enumeration value="SUP"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="dateAction" type="dateType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="adresseType">
		<xsd:annotation>
			<xsd:documentation>Adresses type, telephones, ...</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="adresse1" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="55"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="adresse2" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="55"/>
					</xsd:restriction>
