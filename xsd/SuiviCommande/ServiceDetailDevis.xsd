	<xsd:complexType name="articleType">
		<xsd:annotation>
			<xsd:documentation>Article</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="codeArticle">
				<xsd:annotation>
					<xsd:documentation>Code de l'article</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="25"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="libelleArticle" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Designation de l'article</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="quantite" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Quantite, decimal</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="unite" default="UN">
				<xsd:annotation>
					<xsd:documentation>Unite dans laquelle la quantite est exprimee</xsd:documentation>
