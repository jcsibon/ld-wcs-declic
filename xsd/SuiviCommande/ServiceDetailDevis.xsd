				</xsd:annotation>
			</xsd:element>
			<xsd:element name="puCatalTTC" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prix Unitaire Vente TTC Hors remise, hors promotion</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="puVenteHT" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prix Unitaire Vente Brut, apres promo  Hors Taxe</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="puVenteTTC" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prix Unitaire Vente Brut apres promo TTC</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="puNetHT" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prix Unitaire Vente Net avec promo et remises Hors Taxe</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="puNetTTC" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prix Unitaire Vente Net avec promo et remises TTC</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="puAchatHT" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prix Unitaire Achat (PMA), Ferrage, ..., Hors Taxe</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="criteres" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Critere, Famille, ...</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codeCritere" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Famille, ...</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="valeurCritere" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Valeur de la famille, ...</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="cameleonEnteteType">
		<xsd:annotation>
			<xsd:documentation>Modele pour les Donnees Techniques</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="isClCom">
				<xsd:annotation>
					<xsd:documentation>Entete de commande SCC</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="numCom">
							<xsd:annotation>
								<xsd:documentation>Numero de commande</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="25"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="typeCommande">
							<xsd:annotation>
								<xsd:documentation>Type de Commandes (C : Commande, D: Devis)</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string"/>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="etatCommande" default="" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>V : Valide, sinon, vide</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string"/>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="etatISIA" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>0: Dispo, 1: en modification, 2, en creation</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string"/>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="refClient">
							<xsd:annotation>
								<xsd:documentation>Reference du client</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="35"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="nomClient" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Nom du client</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string"/>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="refDevise" type="xsd:string"/>
						<xsd:element name="refLangue" type="xsd:string"/>
						<xsd:element name="montantCde" type="xsd:decimal">
							<xsd:annotation>
								<xsd:documentation>Montant total de la commande</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="commentCde" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Commentaire associe a la commande</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="modChifTarif" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Mode de chiffrage</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="refReseau" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Reference reseau en lien avec IsReseau</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="valide" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Valididite de l'enregistrement : 0 = OK, 1 = supprime</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
