			<xsd:documentation>Modele pour les dates</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="annee">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1000"/>
						<xsd:maxInclusive value="2099"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="mois">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="12"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="jour">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="31"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="heure" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="23"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="minute" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="59"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="seconde" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="59"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="infosWebType">
		<xsd:annotation>
			<xsd:documentation>Modele pour les informations relatives au compte web</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="idWeb">
				<xsd:annotation>
					<xsd:documentation>identifiant du web</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="dateCreation" type="dateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>date de creation du compte web</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dateDesactivation" type="dateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>date de desactivation du compte web</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dateDerniereConnexion" type="dateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>date de derniere connexion au compte web</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="nombreConnexion" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>nombre de connexions au compte web</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dateDerniereModification" type="dateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>date de derniere modification du compte web</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="magasinPrefere" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>magasin prefere declare du compte web</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="livraisonType">
		<xsd:annotation>
			<xsd:documentation>Permet de definir les criteres de la livraison</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="dateDemandee" type="dateType">
				<xsd:annotation>
					<xsd:documentation>Date demandee de livraison</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dateAuPlusTard" type="dateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date au plus tard de livraison</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dateAuPlusTot" type="dateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date au plus tot de livraison</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dateAuPlusTotTheorique" type="dateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date calculee, au plus tot de livraison</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="livraisonPartielle" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Oui / Non, pour le niveau ou l'on est</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="zoneStockageReceptrice" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Informations pour la plate-forme de reception - Client livre</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codeZone" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Code de la zone de stockage</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="5"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="libelleZone">
							<xsd:annotation>
								<xsd:documentation>Designation de cette zone</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="25"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="codeTournee" minOccurs="0">
				<xsd:complexType/>
			</xsd:element>
			<xsd:element name="transport" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Informations propres au mode d'acheminement</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codeTransporteur" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Code de l'artisan Transporteur pour Export</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="25"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="codeTransport">
							<xsd:annotation>
								<xsd:documentation>Code du mode de transport (C)lient, (T)ransporteur</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:length value="1"/>
									<xsd:enumeration value="C"/>
									<xsd:enumeration value="T"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="libelleTransport" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Libelle du mode de transport</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="25"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="typePort" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Type de port</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="10"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="distance" type="xsd:integer" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Distance de l'etape</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="commentaire" type="commentaireType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Commentaires lies a la livraison</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="partenaireType">
		<xsd:annotation>
			<xsd:documentation>Definition d'un intervenant, interne ou externe, </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence minOccurs="0">
			<xsd:element name="enseigneCommerciale" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Nom de l'enseigne (Pyramide commerciale)</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="25"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="entiteCommerciale" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Nom de l'entite commerciale (Pyramide commerciale)</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="25"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="identifiant" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Si le site est reference dans une table, code du site, facultatif</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="coordonnees" type="coordonneesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Nom, Raison sociale, codes, ...</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contact" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Contact</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codeContact" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="10"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="coordonnees" type="coordonneesType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Nom, Raison sociale, codes, ...</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="gerant" type="coordonneesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Gerant, Directeur ou responsable</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="reglementType">
		<xsd:annotation>
			<xsd:documentation>Modele pour les reglements</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="codeReglement">
				<xsd:annotation>
					<xsd:documentation>Code du reglement</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="libelleReglement" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Libelle du reglement</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="dateReglement" type="dateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date prevue d'echeance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="valorisationType">
		<xsd:annotation>
			<xsd:documentation>Modele pour la valorisation</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="typeValorisation">
				<xsd:annotation>
					<xsd:documentation>Achat, Install, Prestation,Vente</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="Achat"/>
						<xsd:enumeration value="Install"/>
						<xsd:enumeration value="Vente"/>
						<xsd:enumeration value="Prestation"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="montantHT" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Montant HTnet, remises deduites</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="remise" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Remise optionnelle</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="montantHT" type="xsd:decimal">
							<xsd:annotation>
								<xsd:documentation>Montant HT de la remise</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="montantTTC" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Montant TTC de la Remise</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="montantRemise" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Montant TTC de la Remise</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="typeRemise" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Type de remise</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="10"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="tauxRemise" type="xsd:decimal">
							<xsd:annotation>
								<xsd:documentation>Taux de la remise, Decimal</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="promotion" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>promotion</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="montantHT" type="xsd:decimal">
							<xsd:annotation>
								<xsd:documentation>Montant HT de la promotion</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="montantTTC" type="xsd:decimal">
							<xsd:annotation>
								<xsd:documentation>Montant TTC de la promotion</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="typePromotion" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Type de promotion</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="10"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="codePromotion" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Code de promotion</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="10"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="libellePromotion" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Libelle de promotion</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="50"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="tauxPromotion" type="xsd:decimal">
							<xsd:annotation>
								<xsd:documentation>Taux de la promotion, Decimal</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="escompte" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Excompte optionnelle</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="tauxEscompte">
							<xsd:annotation>
								<xsd:documentation>Taux d'escompte</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:decimal">
									<xsd:maxInclusive value="100"/>
									<xsd:minInclusive value="0"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="montantEscompte" type="xsd:decimal">
							<xsd:annotation>
								<xsd:documentation>Montant escompte</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="tva" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>tva Totales</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="montantHT" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Montant HTnet, concerne par la TVA</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="tauxTVA" default="19.6">
							<xsd:annotation>
								<xsd:documentation>Taux de tva (decimal)</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:decimal">
									<xsd:minInclusive value="0"/>
									<xsd:maxInclusive value="100"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="montantTVA" type="xsd:decimal">
							<xsd:annotation>
								<xsd:documentation>Montant tva</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="taxes" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Taxes diverses</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="montantHT" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Montant HTnet, concerne par la taxe</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="tauxTaxe" default="19.6">
							<xsd:annotation>
								<xsd:documentation>Taux (decimal)</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:decimal">
									<xsd:minInclusive value="0"/>
									<xsd:maxInclusive value="100"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="montantTaxes" type="xsd:decimal">
							<xsd:annotation>
								<xsd:documentation>Montant Taxe</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="montantTTC" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Montant ht Produits</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="acompteRegle" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Accompte deja regle</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
