				<xsd:annotation>
					<xsd:documentation>Texte libre</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="254"/>
						<xsd:whiteSpace value="preserve"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="libelle3Commentaire" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Texte libre</xsd:documentation>
