<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2005 rel. 3 U (http://www.altova.com) by DOSI02 (LAPEYRE) -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" attributeFormDefault="unqualified">
	<xsd:element name="pivotDocument">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="enteteDocument">
					<xsd:annotation>
						<xsd:documentation>Caracteristiques generales du document</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="versionPivot" fixed="5.0">
								<xsd:annotation>
									<xsd:documentation>Version du message ici = 5.0</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string"/>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="numeroMessage" type="xsd:long">
								<xsd:annotation>
									<xsd:documentation>No du message, compteur,unique quelque soit le message</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="dateCreationMessage" type="dateType">
								<xsd:annotation>
									<xsd:documentation>Date de creation du message</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="emetteur">
								<xsd:annotation>
									<xsd:documentation>Emetteur du message, Obligatoire, balise vide si problemes de connection avec Base de donnees en extraction, panier sans ligne dans ce cas</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:complexContent>
										<xsd:extension base="partenaireType">
											<xsd:sequence>
												<xsd:element name="applicationSource">
													<xsd:annotation>
														<xsd:documentation>Nom de l'applicatif source (pour transcodification, ...)</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="25"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
										</xsd:extension>
									</xsd:complexContent>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="exclusionsCibles" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>flags d'application cible</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="action">
								<xsd:annotation>
									<xsd:documentation>Creation, Modification, Suppression</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:complexContent>
										<xsd:extension base="actionType">
											<xsd:sequence>
												<xsd:element name="acteurAction" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Celui qui a fait l'action</xsd:documentation>
													</xsd:annotation>
													<xsd:complexType>
														<xsd:complexContent>
															<xsd:extension base="coordonneesType">
																<xsd:sequence>
																	<xsd:element name="matriculeActeurAction" type="xsd:string">
																		<xsd:annotation>
																			<xsd:documentation>Code (Lapeyre) ou matricule (K par K) vendeur</xsd:documentation>
																		</xsd:annotation>
																	</xsd:element>
																</xsd:sequence>
															</xsd:extension>
														</xsd:complexContent>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="siteAction" type="partenaireType" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Dans Pyramide commerciale</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:extension>
									</xsd:complexContent>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosVente" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Vendeur et son site de rattachement</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="vendeur" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Vendeur</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:complexContent>
													<xsd:extension base="coordonneesType">
														<xsd:sequence>
															<xsd:element name="matriculeVendeur" type="xsd:string"/>
														</xsd:sequence>
													</xsd:extension>
												</xsd:complexContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="siteVendeur" type="partenaireType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>site de rattachement du vendeur</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="codeDevise" default="EUR">
								<xsd:annotation>
									<xsd:documentation>Code de la devise</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string"/>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="export" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Exportation O/N</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="O"/>
										<xsd:enumeration value="N"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="remiseTheorique" type="xsd:decimal" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Remise de base du client (10.00, 15.00, ...)</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="infosCreation">
								<xsd:annotation>
									<xsd:documentation>Dates, references, ...</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="typeDocument">
											<xsd:annotation>
												<xsd:documentation>Devis, Commande, Contrat</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string"/>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="sousTypeDocument" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>REC(lamation), POS (Installation), ...</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string"/>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="universDocument" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Univers principal du contrat, ... (K par K)</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string"/>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="numeroDocument">
											<xsd:annotation>
												<xsd:documentation>No Transaction, commande, devis, ....</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="25"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="numeroDocumentWeb" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="40"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="siteCreateurDocument" type="partenaireType">
											<xsd:annotation>
												<xsd:documentation>Site du document d'origine, plateforme pour K par K</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="siteLogistique" type="partenaireType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Depot K par K</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="siteVente" type="partenaireType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Magasin K par K</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="dateCreationDocument" type="dateType">
											<xsd:annotation>
												<xsd:documentation>Date initiale de creation du document (date commande, date contrat)</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="codeMotifAnnul" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string"/>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="infosStatutDocument" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Statut</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="codeStatutDocument">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:length value="3"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="libelleStatutDocument">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="30"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="dateStatut" type="dateType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Date de modification du statut du document</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosDocumentPrecedent" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Document  precedent (Devis, Commande, Facture, ...)</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="typeDocumentPrecedent">
											<xsd:annotation>
												<xsd:documentation>Devis, contrat, Commande, facture</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="250"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="sousTypeDocumentPrecedent" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>REC(lamation), POS (Installation), ...</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string"/>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="numeroDocumentPrecedent">
											<xsd:annotation>
												<xsd:documentation>No document  precedent</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="25"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="vendeurDocumentPrecedent" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Vendeur createur du document precedent</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:complexContent>
													<xsd:extension base="coordonneesType">
														<xsd:sequence>
															<xsd:element name="matriculeActeurAction" type="xsd:string">
																<xsd:annotation>
																	<xsd:documentation>Code (Lapeyre) ou matricule (K par K) vendeur</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
														</xsd:sequence>
													</xsd:extension>
												</xsd:complexContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="siteCreateurDocumentPrecedent" type="partenaireType">
											<xsd:annotation>
												<xsd:documentation>Sitedu document precedent</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="dateCreationDocumentPrecedent" type="dateType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Date initiale de creation du document  precedent</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosOrigineCommerciale" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Origine de la transaction commerciale (transformation du devis en commande...)</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="vendeurOrigineCommerciale" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Vendeur à l'origine de la transaction commerciale (transformation du devis en commande...)</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:complexContent>
													<xsd:extension base="coordonneesType">
														<xsd:sequence>
															<xsd:element name="matriculeVendeur" type="xsd:string">
																<xsd:annotation>
																	<xsd:documentation>Code (Lapeyre) ou matricule (K par K) vendeur</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
														</xsd:sequence>
													</xsd:extension>
												</xsd:complexContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="siteOrigineCommerciale" type="partenaireType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Site à l'origine de la transaction commerciale (transformation du devis en commande...)</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="dateOrigineCommerciale" type="dateType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Date de la transaction commerciale (transformation du devis en commande...)</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosProspection" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Demarchage</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="codeSource">
											<xsd:annotation>
												<xsd:documentation>TA P, JPO, ....</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="3"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="teleActeur" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Operateur Call Center</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:complexContent>
													<xsd:extension base="coordonneesType">
														<xsd:sequence>
															<xsd:element name="matriculeTeleActeur" type="xsd:string"/>
														</xsd:sequence>
													</xsd:extension>
												</xsd:complexContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="siteTeleActeur" type="partenaireType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>site Call Center</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="numeroCouponUtilise" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:minLength value="1"/>
										<xsd:maxLength value="20"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="infosProjet" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Projet</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="numeroProjet" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Specifique Contrats K par K</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="20"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="codeAttributionRelance" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>M(agasin), S(iege), ...</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="8"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="codeTypePose" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Lapeyre, Artisan, ...</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="5"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="propositionFinancement" type="xsd:boolean" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Oui / Non</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="budget" type="xsd:decimal" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Budget</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="codeRepPropFinancement" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>code Reponse</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="5"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="dateProjet" type="dateType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Date </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="dateRelance" type="dateType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Date Relance</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="commentaireProjet" type="commentaireType" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosPromotionDocument" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="codePromotionAppliquee">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="45"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="libellePromotionAppliquee">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="45"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="dateStatistique" type="dateType" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Date statistique (K par K) </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="infosArtisan" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Si client professionnel</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="carteProfessionnelle" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Carte pro</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="20"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="numeroCommandeArtisan" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Reference de la commande de l'artisan</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="20"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="numeroChantierArtisan" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>No de chantier de l'Artisan</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
													<xsd:maxLength value="20"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosDispos" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="statutDispoCommande" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:length value="3"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="dateMADMagasin" type="dateType" minOccurs="0"/>
										<xsd:element name="dateDisponibiliteReelle" type="dateType" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosPrestationsAtelier" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="dateEntreeAtelier" type="dateType"/>
										<xsd:element name="dateSortieAtelier" type="dateType"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosEnlevement" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="dateEnlevementClient" type="dateType" minOccurs="0"/>
										<xsd:element name="modeEnlevement" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>G(lobal), P(artiel)</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:enumeration value="P"/>
													<xsd:enumeration value="G"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="siteEnlevement" type="partenaireType" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="infosEtape" minOccurs="0" maxOccurs="unbounded">
