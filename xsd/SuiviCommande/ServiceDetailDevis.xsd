					<xsd:sequence>
						<xsd:element name="X" type="xsd:decimal"/>
						<xsd:element name="Y" type="xsd:decimal"/>
						<xsd:element name="typeGeocodage" type="xsd:string" minOccurs="0"/>
						<xsd:element name="scoreGeocodage" minOccurs="0">
